{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "preinstall": {},
    "prebuild": {
      "dependsOn": ["^prebuild", "preinstall"]
    },
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": ["dist/**"]
    },
    "prelint": {
      "dependsOn": ["^prelint", "build"]
    },
    "lint": {
      "dependsOn": ["prelint"]
    }
  }
}
