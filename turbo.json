{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "preinstall": {
      "outputMode": "new-only"
    },
    "prebuild": {
      "dependsOn": ["^prebuild", "preinstall"],
      "outputMode": "new-only"
    },
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "pretest": {
      "outputMode": "new-only"
    }
  }
}
