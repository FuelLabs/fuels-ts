{
  "name": "@fuel-ts/abi-typegen",
  "version": "0.22.0",
  "description": "Generates Typescript definitions from Sway ABI Json files",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "typedocMain": "./src/index.ts",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "bin": {
    "fuels-typegen": "dist/bin.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./dist/cli": "./dist/cli.js"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "default": "./dist/index.mjs"
      },
      "./dist/cli": "./dist/cli.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "lodash.upperfirst": "^4.3.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@jest/types": "28.1.0",
    "@types/glob": "^8.0.0",
    "@types/lodash.upperfirst": "^4.3.7",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.11.9",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^10.0.13",
    "@types/yargs": "^17.0.13",
    "forc-bin": "workspace:*",
    "jest-text-transformer": "^1.0.4",
    "sinon": "^14.0.1",
    "ts-mock-imports": "^1.3.8",
    "ts-node": "^10.9.1",
    "tsup": "^5.12.9",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tsup --dts",
    "build:fixtures": "ts-node ./test/fixtures/buildAll.ts",
    "test": "jest test --coverage",
    "test:watch": "jest test --watch --coverage"
  }
}
