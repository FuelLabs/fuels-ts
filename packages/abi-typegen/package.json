{
  "name": "@fuel-ts/abi-typegen",
  "version": "0.44.1",
  "description": "Generates Typescript definitions from Sway ABI Json files",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "bin": {
    "fuels-typegen": "typegen.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "require": "./dist/cli.js",
      "import": "./dist/cli.mjs",
      "types": "./dist/cli.d.ts"
    },
    "./runTypegen": {
      "require": "./dist/runTypegen.js",
      "import": "./dist/runTypegen.mjs",
      "types": "./dist/runTypegen.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/index.ts"
      ],
      "cli": [
        "./src/cli.ts"
      ],
      "runTypegen": [
        "./src/runTypegen.ts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "index": [
          "./dist/index.d.ts"
        ],
        "cli": [
          "./dist/cli.d.ts"
        ],
        "runTypegen": [
          "./dist/runTypegen.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "pretest": "pnpm forc build -p test/fixtures/forc-projects"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@ethersproject/bytes": "^5.7.0",
    "@fuel-ts/utils": "workspace:*",
    "@fuel-ts/versions": "workspace:*",
    "commander": "^9.4.1",
    "glob": "^10.2.6",
    "handlebars": "^4.7.7",
    "lodash.uniq": "^4.5.0",
    "lodash.upperfirst": "^4.3.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/lodash.uniq": "^4.5.7",
    "@types/lodash.upperfirst": "^4.3.7",
    "@types/mkdirp": "^1.0.2",
    "@types/rimraf": "^3.0.2",
    "forc-bin": "workspace:*"
  }
}
