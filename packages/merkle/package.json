{
  "name": "@fuel-ts/merkle",
  "version": "0.38.1",
  "description": "",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
