{
  "name": "@fuel-ts/address",
  "version": "0.42.0",
  "description": "Utilities for encoding and decoding addresses",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "exports": {
    ".": "./src/index.ts",
    "./configs": "./src/configs.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*"
      ],
      "configs": [
        "./src/configs.ts"
      ]
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "typesVersions": {
      "*": {
        "index.d.ts": [
          "./dist/index.d.ts"
        ],
        "configs": [
          "./dist/configs.d.ts"
        ]
      }
    },
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./configs": {
        "import": "./dist/configs.mjs",
        "require": "./dist/configs.js",
        "types": "./dist/configs.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@fuel-ts/interfaces": "workspace:*",
    "@fuel-ts/keystore": "workspace:*",
    "@fuel-ts/versions": "workspace:*",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/sha2": "^5.7.0",
    "bech32": "^2.0.0"
  },
  "devDependencies": {
    "@fuel-ts/testcases": "workspace:*"
  },
  "scripts": {
    "build": "tsup --dts",
    "postbuild": "pnpm run inject:secondary:entrypoints",
    "inject:secondary:entrypoints": "run-p inject:*",
    "inject:configs": "echo \"import './configs.d.ts';\" >> ./dist/index.d.ts"
  }
}
