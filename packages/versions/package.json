{
  "name": "@fuel-ts/versions",
  "version": "0.101.0",
  "description": "Validates supported versions of the Fuel toolchain",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "bin": {
    "fuels-versions": "versions.ts"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "engines": {
    "node": "^18.20.3 || ^20.0.0 || ^22.0.0"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./cli": {
      "types": "./src/cli.ts",
      "require": "./src/cli.ts",
      "import": "./src/cli.ts"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
      "fuels-versions": "versions.js"
    },
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
      },
      "./cli": {
        "types": "./dist/cli.d.ts",
        "require": "./dist/cli.js",
        "import": "./dist/cli.mjs"
      }
    },
    "typesVersions": {
      "*": {
        "cli": [
          "./dist/cli.d.ts"
        ]
      }
    }
  },
  "typesVersions": {
    "*": {
      "cli": [
        "./src/cli.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "tsx ./scripts/rewriteVersions.ts",
    "build": "tsup",
    "postbuild": "tsx ../../scripts/postbuild.ts"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "4",
    "cli-table": "0.3.11"
  },
  "devDependencies": {
    "@types/cli-table": "0.3.4"
  }
}
