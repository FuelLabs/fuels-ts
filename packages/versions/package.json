{
  "name": "@fuel-ts/versions",
  "version": "0.41.0",
  "description": "Validates supported versions of the Fuel toolchain",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "bin": {
    "fuels-versions": "dist/bin.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "require": "./dist/cli.js",
      "import": "./dist/cli.mjs",
      "types": "./dist/cli.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/index.ts"
      ],
      "cli": [
        "./src/cli.ts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "index.d.ts": [
          "./dist/index.d.ts"
        ],
        "cli": [
          "./dist/cli.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "ts-node ./scripts/rewriteVersions.ts",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "4",
    "cli-table": "^0.3.11",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@internal/tsup": "workspace:*",
    "@types/cli-table": "^0.3.1",
    "@types/semver": "^7.3.13"
  }
}
