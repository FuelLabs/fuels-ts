{
  "name": "@fuel-ts/wallet",
  "version": "0.39.1",
  "description": "",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/cjs/src/index.d.ts"
    },
    "./configs": {
      "require": "./dist/cjs/src/configs.js",
      "import": "./dist/esm/src/configs.js",
      "types": "./dist/cjs/src/configs.d.ts"
    },
    "./test-utils": {
      "import": "./dist/esm/test/utils/index.js",
      "require": "./dist/cjs/test/utils/index.js",
      "types": "./dist/cjs/test/utils/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/cjs/src/index.d.ts"
      ],
      "test-utils": [
        "./dist/cjs/test/utils/index.d.ts"
      ],
      "configs": [
        "./dist/cjs/src/configs.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "run-p 'build:* --watch'",
    "build": "run-p build:*",
    "build:cjs": "tsc -m commonjs -p tsconfig.build.json --outDir dist/cjs",
    "build:esm": "tsc -m esnext -p tsconfig.build.json --outDir dist/esm",
    "postbuild": "pnpm run inject:secondary:entrypoints",
    "inject:secondary:entrypoints": "run-p inject:*",
    "inject:configs": "echo \"import './configs.d.ts';\" >> ./dist/cjs/src/index.d.ts",
    "inject:test-utils": "echo \"import '../test/utils/index.d.ts';\" >> ./dist/cjs/src/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@ethersproject/bytes": "^5.7.0",
    "@fuel-ts/abi-coder": "workspace:*",
    "@fuel-ts/address": "workspace:*",
    "@fuel-ts/hasher": "workspace:*",
    "@fuel-ts/hdwallet": "workspace:*",
    "@fuel-ts/interfaces": "workspace:*",
    "@fuel-ts/math": "workspace:*",
    "@fuel-ts/mnemonic": "workspace:*",
    "@fuel-ts/providers": "workspace:*",
    "@fuel-ts/signer": "workspace:*",
    "@fuel-ts/transactions": "workspace:*"
  },
  "devDependencies": {
    "@fuel-ts/address": "workspace:*",
    "@fuel-ts/keystore": "workspace:*",
    "@fuel-ts/testcases": "workspace:*"
  }
}
