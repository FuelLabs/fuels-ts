{
  "name": "fuels-ts",
  "version": "0.0.1",
  "description": "Fuel TS SDK",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14.18.0",
    "npm": ">=8.0.0"
  },
  "workspaces": [
    "./packages/abi-coder",
    "./packages/merkle-shared",
    "./packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:coverage": "npm run test:coverage --workspaces --if-present",
    "lint": "npx eslint . --ext .ts && npx prettier --check packages",
    "prettier-format": "prettier --config .prettierrc 'packages/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FuelLabs/fuels-ts.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/FuelLabs/fuels-ts/issues"
  },
  "homepage": "https://github.com/FuelLabs/fuels-ts#readme",
  "devDependencies": {
    "@typechain/ethers-v5": "^6.0.5",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^8.2.3",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lerna": "^4.0.0",
    "markdownlint": "^0.23.1",
    "markdownlint-cli": "^0.27.1",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.4"
  }
}
