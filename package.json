{
  "name": "fuels-ts",
  "version": "0.0.1",
  "description": "Fuel TS SDK",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14.19.0",
    "pnpm": ">=7"
  },
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "ci:test": "run-s services:run test services:clean",
    "ci:test-coverage": "run-s services:run test:coverage services:clean",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collectCoverage",
    "lint": "run-s lint:check prettier:check",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "pnpm lint:check -- --fix",
    "prettier:check": "prettier --check packages",
    "prettier:format": "prettier --write packages",
    "services:run": "make services-run",
    "services:clean": "make services-clean",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FuelLabs/fuels-ts.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FuelLabs/fuels-ts/issues"
  },
  "homepage": "https://github.com/FuelLabs/fuels-ts#readme",
  "devDependencies": {
    "@jest/types": "^28.0.2",
    "@qiwi/multi-semantic-release": "^3.17.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semrel-extra/npm": "^1.2.0",
    "@typechain/ethers-v5": "^6.0.5",
    "@types/jest": "^27.5.0",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "dotenv": "^9.0.2",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.2.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.4.3",
    "lerna": "^4.0.0",
    "markdownlint": "^0.23.1",
    "markdownlint-cli": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "open": "^8.4.0",
    "prettier": "^2.5.1",
    "ts-generator": "^0.1.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tsup": "^5.12.6",
    "turbo": "^1.2.6",
    "typechain": "^4.0.3",
    "typedoc": "^0.22.11",
    "typedoc-just-the-docs-theme": "^0.0.1",
    "typedoc-monorepo-link-types": "^0.0.2",
    "typedoc-plugin-markdown": "^3.11.12",
    "typedoc-plugin-sourcefile-url": "^1.0.6",
    "typescript": "^4.5.2"
  }
}
